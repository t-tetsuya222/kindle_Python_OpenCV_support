   "source": [
    "import cv2\n",
    "\n",
    "img = cv2.imread(\"src/Lena.jpg\",0)\n",
    "\n",
    "# ラプラシアンフィルター\n",
    "img_lap = cv2.Laplacian(img, cv2.CV_32F)\n",
    "# uint8に変換\n",
    "img_lap = cv2.convertScaleAbs(img_lap)\n",
    "cv2.imshow(\"laplacian\", img_lap)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
